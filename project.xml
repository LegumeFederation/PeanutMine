<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="peanutmine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>

  <sources>

    <!-- LIS/PEANUTBASE CHADO -->

    <!-- LIS chado genomics - all the LIS species with genes -->
    <source name="lis-genomics" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="lis-chado"/>
      <property name="organisms" value="3818_Tifrunner 130453_V14167 130454_K30076"/>
      <property name="dataSetTitle" value="LIS chado genomic data"/>
      <property name="dataSourceName" value="LIS chado"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.SequenceProcessor"/>
    </source>

    <!-- Peanutbase chado genetics -->
    <source name="peanutbase-genetics" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="peanutbase-chado"/>
      <property name="organisms" value="3818_Tifrunner 130453_V14167 130454_K30076"/>
      <property name="dataSetTitle" value="Peanutbase chado genetic data"/>
      <property name="dataSourceName" value="Peanutbase chado"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GeneticProcessor"/>
    </source>

    <!-- LIS chado featureprop -->
    <source name="lis-featureprop" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="lis-chado"/>
      <property name="organisms" value="3818_Tifrunner 130453_V14167 130454_K30076"/>
      <property name="dataSetTitle" value="LIS chado featureprop data"/>
      <property name="dataSourceName" value="LIS chado"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.FeaturePropProcessor"/>
    </source>

    <!-- Peanutbase chado featureprop -->
    <source name="peanutbase-featureprop" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="peanutbase-chado"/>
      <property name="organisms" value="3818_Tifrunner 130453_V14167 130454_K30076"/>
      <property name="dataSetTitle" value="Peanutbase chado featureprop data"/>
      <property name="dataSourceName" value="Peanutbase chado"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.FeaturePropProcessor"/>
    </source>

    <!-- LIS chado gene families -->
    <source name="lis-genefamilies" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="lis-chado"/>
      <property name="organisms" value="3818_Tifrunner 130453_V14167 130454_K30076"/>
      <property name="homologue.organisms" value="3818_Tifrunner 3821_ICPL87119 3827_CDCFrontier 3827_ICC4958 3847_Williams82 3871_Tanjil 3880_A17 3885_G19833 3914_Gyeongwon 3920_IT97K-499-35 34305_MG20 57577_MilvusB 130453_V14167 130454_K30076 157791_VC1973A"/>
      <property name="dataSetTitle" value="LIS chado gene family data"/>
      <property name="dataSourceName" value="LIS chado"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.GeneFamilyProcessor"/>
    </source>

    <!-- LIS chado homology - based on phylotree - to pick up Arabidopsis homologues -->
    <source name="lis-homology" type="legfed-chado-db" dump="true">
      <property name="phytozome.version" value="phytozome_10_2"/>
      <property name="source.db.name" value="lis-chado"/>
      <property name="organisms" value="3818_Tifrunner 130453_V14167 130454_K30076"/>
      <property name="homologue.organisms" value="3702_Col0"/>
      <property name="dataSetTitle" value="LIS chado phylotree data"/>
      <property name="dataSourceName" value="LIS chado"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.HomologyProcessor"/>
    </source>
    
    <!-- LIS chado + reactome file -->
    <source name="lis-reactome-file" type="legfed-chado-db" dump="true">
      <property name="source.db.name" value="lis-chado"/>
      <property name="reactome.filename" location="/home/shokin/legfed-intermine-data/plant-reactome/reactome.txt"/>
      <property name="organisms" value="3818_Tifrunner 130453_V14167 130454_K30076"/>
      <property name="dataSetTitle" value="Plant Reactome data"/>
      <property name="dataSourceName" value="Plant Reactome file"/>
      <property name="converter.class" value="org.intermine.bio.dataconversion.ChadoDBConverter"/>
      <property name="processors" value="org.intermine.bio.dataconversion.ReactomeProcessor"/>
    </source>

    <!-- PEANUT NON-CHADO -->

    <!-- genetic marker genomic locations from GFF files -->
    <source name="peanut-geneticmarker-gff" type="legfed-geneticmarker-gff" dump="true">
      <property name="src.data.dir" location="/home/shokin/legfed-intermine-data/peanut-markers"/>
    </source>

    <!-- duranensis to ipaensis synteny from GFF file -->
    <source name="peanut-synteny-gff" type="legfed-synteny-gff" dump="true">
      <property name="src.data.dir" location="/home/shokin/legfed-intermine-data/peanut-synteny"/>
    </source>
    
    <!-- peanutbase expression -->
    <source name="peanut-expression" type="legfed-expression" dump="true">
      <property name="src.data.dir" location="/home/shokin/legfed-intermine-data/peanut-expression"/>
    </source>

    <!-- ONTOLOGIES -->

    <!-- sequence ontology -->
    <source name="so" type="so">
      <property name="src.data.file" location="../../bio/sources/so/so.obo"/>
    </source>

    <!-- gene ontology -->
    <source name="go" type="go">
        <property name="src.data.file" location="/home/shokin/legfed-intermine-data/gene-ontology/go-basic.obo"/>
    </source>

    <!-- trait ontology -->
    <source name="to" type="legfed-trait-ontology">
      <property name="src.data.file" location="/home/shokin/plant-trait-ontology/to.obo"/>
      <property name="ontology.prefix" value="TO"/>
    </source>

    <!-- plant ontology -->
    <source name="po" type="legfed-plant-ontology">
      <property name="src.data.file" location="/home/shokin/legfed-intermine-data/plant-ontology/po.obo"/>
      <property name="ontology.prefix" value="PO"/>
    </source>

  </sources>

  <post-processing>

    <!-- source-specific post-processing -->
    <post-process name="do-sources" dump="true"/>

    <!-- legfed -->
    <post-process name="create-go-annotations" dump="true"/>
    <post-process name="create-protein-gene-references" dump="true"/>
    <post-process name="create-protein-domain-references" dump="true"/>
    <post-process name="create-ontology-parent-annotations" dump="true"/>
    <post-process name="populate-publications" dump="true"/>
    <post-process name="populate-linkagegroup-lengths" dump="true"/>
    <post-process name="populate-qtl-linkagegroups" dump="true"/>
    <post-process name="populate-gene-spanning-qtls" dump="true"/>
    <post-process name="populate-interpro-data" dump="true"/>
    
    <!-- add features -->
    <post-process name="create-intron-features"/>
    <post-process name="create-intergenic-region-features"/>
    <post-process name="create-gene-flanking-features"/> 
   
    <!-- create various references and collections -->
    <post-process name="populate-located-features"/>
    <post-process name="populate-child-features"/>
    <post-process name="create-references" />
    <post-process name="create-chromosome-locations-and-lengths"/>
    <post-process name="transfer-sequences"/>

    <!-- views and indexes, after all data is set -->
    <post-process name="create-location-overlap-index"/>
    <post-process name="create-overlap-view" />
    <post-process name="create-attribute-indexes"/>
    <post-process name="summarise-objectstore"/>
    <post-process name="create-autocomplete-index"/>
    <post-process name="create-search-index"/>

  </post-processing>

</project>
